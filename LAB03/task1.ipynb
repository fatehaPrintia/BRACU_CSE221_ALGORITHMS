    "graph={}\n",
    "file=open(\"input.txt\",\"r\")\n",
    "total=int(file.readline())\n",
    "for i in range(1,total+1):\n",
    "    graph[str(i)]=file.readline().split()[1:]\n",
    "out=open('output1.txt', 'a')\n",
    "for k in graph:\n",
    "    out.write(f\"{k}:\")\n",
    "    out.write(f\"{graph[k]}\")\n",
    "    out.write(\"\\n\")\n",
    "file.close()\n",
    "print(graph)"
